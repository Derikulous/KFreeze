<%= form_for @user do |f| %>

<fieldform>
  <div>
    <%= f.label :user_name %><br>
    <%= f.text_field :user_name %>
  </div>
    <div>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.label :password_confirmation %><br>
    <%= f.password_field :password_confirmation %>
  </div>
<%= f.submit %>
<% end %>
</fieldform>
