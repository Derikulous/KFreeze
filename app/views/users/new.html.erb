<% provide(:title, 'Sign up') %>
<center><h1>Sign Up</h1></center>


<div class="row">
  <div class="span6 offset3">
    <%= form_for(@user) do |f| %>
    <%= render 'shared/error_messages', object: f.object %><br>
    <%= f.submit "Create account.", class: "btn btn-success btn-large" %>
    <% end %>
  </div>
</div>
